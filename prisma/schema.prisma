generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int    @id @default(autoincrement())
  nome  String @db.VarChar(100)
  email String @unique(map: "email") @db.VarChar(100)
  senha String @db.VarChar(255)
}
